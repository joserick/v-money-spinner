  "description": "A component with input spinner and currency mask.",
  "main": "./dist/v-money-spinner.umd.js",
  "module": "./dist/v-money-spinner.es.js",
  "exports": {
    ".": {
      "import": "./dist/v-money-spinner.es.js",
      "require": "./dist/v-money-spinner.umd.js"
    }
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",

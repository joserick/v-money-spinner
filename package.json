  "author": "Jose Erick Carreon Gomez <joserick.92@gmail.com> (https://github.com/joserick/v-money-spinner)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joserick/v-money-spinner/issues"
  },
  "homepage": "https://github.com/joserick/v-money-spinner#readme",
  "dependencies": {
    "v-money3": "^3.23.0",
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.0.0",
    "ts-standard": "^12.0.2",
    "typescript": "^4.9.5",
    "vite": "^4.1.0",
    "vite-plugin-dts": "^2.1.0",
    "vue-tsc": "^1.0.24"
  },
  "ts-standard": {
    "project": "./tsconfig.json"
  }
